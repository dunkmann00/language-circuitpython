  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "serialport": "9.0.3",
    "@serialport/bindings": "9.0.3"
  },
  "deserializers": {
    "language-circuitpython/BoardView": "deserializeBoardView",
    "language-circuitpython/PlotterView": "deserializePlotterView"
  }
}
